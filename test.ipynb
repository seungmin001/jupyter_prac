    "def selection_sort(a): # 오름차순\n",
    "    n=len(a)\n",
    "    for i in range(0,n-1):\n",
    "        for j in range(i+1,n):\n",
    "            if a[j] < a[i]:\n",
    "                a[i],a[j] = a[j],a[i]\n",
    "                \n",
    "l=[2,5,1,3,4]\n",
    "selection_sort(l)\n",
    "print(l)"
   ]
